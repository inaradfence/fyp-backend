    <div class="main-panel">
      <div class="content-wrapper">
        <div class="col-lg-12 grid-margin stretch-card p-0">
          <div class="card">
            <div class="card-body">
              <h4 class="card-title">Users Collection</h4>
              <div class="table-responsive">
                <table class="table table-striped">
                  <thead>
                    <tr>
                      <th >Sr No</th>
                      <th >First name</th>
                      <th >Email</th>
                      <th >Designation</th>
                      <th style="width:200px;display:block" class="d-block" >Institute</th>
                      <th >Actions</th>
                     
                    </tr>
                  </thead>
                  <tbody>
                    <% let serialNumber = 1; %>
                    <% users.forEach(function(user){ %>
                      <tr>
                        <td><%= serialNumber++ %></td>
                        <td><%= user.firstname %></td>
                         <td><%= user.email %></td>
                        <td><%= user.designation %></td>
                        <td style="width:400px;white-space: normal;" class=""><%= user.institute %></td>
                        <td>
                          <a href="/api/user/<%= user._id %>"  class="btn btn-sm btn-primary">Edit</a>
                          <a href="/api/deleteuser/<%= user._id %>" onclick="return confirm('Are you sure, you want to Delete this ?')" class="btn btn-sm btn-primary"
                         >Delete</a></td>
                        </tr>
                    <% }) %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
